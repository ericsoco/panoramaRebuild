{
  "name": "@stamen/panorama-rebuild",
  "version": "0.1.7",
  "description": "Panorama visualization toolkit",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean:examples && webpack-dev-server --config ./examples/webpack.config.js --hot --inline --content-base examples/public",
    "test": "jest",
    "lint": "eslint ./src",
    "build:examples": "npm run clean:examples && webpack --config ./examples/webpack.config.ghpage.js",
    "clean:examples": "rm -rf ./examples/public/examples.*",
    "publish:examples": "./.bin/publish-examples.sh",
    "build:dist": "npm run clean:dist && webpack --config ./conf/webpack.config.dist.js && webpack --config ./conf/webpack.config.dist.min.js",
    "clean:dist": "rm -rf ./dist/**",
    "clean:dist-modules": "rm -rf ./dist-modules/**",
    "build:dist-modules": "npm run clean:dist-modules && webpack --config ./conf/webpack.config.modules.js && babel ./src/main.js --out-file ./dist-modules/index.js",
    "publish:dist": "./.bin/publish-dist.sh",
    "analyze": "webpack --json --config ./conf/webpack.config.js | analyze-bundle-size",
    "build-modules": "gulp build",
    "dev-modules": "gulp dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stamen/panorama.git"
  },
  "keywords": [
    "visualization",
    "geospatial",
    "maps",
    "mapping",
    "dataviz",
    "geoviz"
  ],
  "author": "stamen",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "scriptPreprocessor": "<rootDir>/lib/preprocessor.js",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "build"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/fbjs"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^1.6.0",
    "json-loader": "^0.5.4",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "topojson": "^1.6.24",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "cartodb-client": "stamen/cartodb-client",
    "d3": "^3.5.16",
    "d3-queue": "^2.0.3",
    "intro.js": "^1.1.1",
    "leaflet": "^0.7.7",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-leaflet": "^0.10.2",
    "react-modal": "^0.6.1"
  }
}
